{
  "expo": {
    "name": "MRH Rider",
    "slug": "rider-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myImages/splash.png",
    "scheme": "riderapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/myImages/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "android": {
      "package": "com.anonymous.riderapp",
      "splash": {
        "image": "./assets/myImages/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#09090b"
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.riderapp",
      "splash": {
        "image": "./assets/myImages/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#09090b"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/myImages/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#09090b"
        }
      ]
    ]
  }
}